                                </select>
                                <label>Seleccione tipo de identificación</label>
                              </div>

                              <div class="col s2"></div>

                              <div class="input-field col s5">
                                <input id="last_name" type="text" class="validate">
                                <label for="last_name">No. de identificación</label>
                              </div>

                              <div class="row"></div>
                              <div class="col s12">
                                <button class="waves-effect waves-light blue darken-1 btn" type="submit" name="action">consultar<i class="material-icons right">send</i></button>
                              </div>
                              </div>
                            </div>   
                              
                    </div>
                </div>
            </div>
        </div>
    </div>

            
